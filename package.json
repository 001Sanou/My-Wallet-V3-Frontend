{
  "name": "My-Wallet-HD",
  "version": "0.5.0",
  "description": "Blockchain.info JavaScript Wallet",
  "homepage": "https://github.com/blockchain/My-Wallet-HD",
  "bugs": {
    "url": "https://github.com/blockchain/My-Wallet-HD/issues"
  },
  "license": "ISC",
  "author": {
    "name": "Blockchain.info",
    "url": "https://blockchain.info"
  },
  "contributors": [
    "Tim Lee"
  ],    
  "main": "wallet.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockchain/My-Wallet-HD.git"
  },
  "scripts": {
    "postinstall": "node_modules/bower/bin/bower install && browserify -r bitcoinjs-lib -s Bitcoin > bitcoinjs-npm.js && cp bower_components/jquery/dist/jquery.min.js . && cd node_modules/bip39 && npm run compile && mv bip39.js ../.. && cd ../.. && cp node_modules/xregexp/xregexp-all.js . && cd node_modules/sjcl && ./configure --with-sha1 && make && cd - && cp node_modules/sjcl/sjcl.js .",
    "test": "node ./node_modules/karma/bin/karma start karma.conf.js"
  },
  "dependencies": {
    "bitcoinjs-lib": "~1.4.3",
    "bip39": "~2.1.0",
    "sjcl": "~1.0.2",
    "xregexp": "~2.0.0"
  },
  "devDependencies": {
    "bower": "~1.3.12",
    "coffee-script": "~1.8.0",
    "karma": "~0.12.31",
    "karma-coffee-preprocessor": "~0.2.1",
    "karma-jasmine": "~0.3.5",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-chrome-launcher": "~0.1.7",
    "phantomjs": "~1.9.15"
  }
}
