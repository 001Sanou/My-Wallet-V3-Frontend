{
  "name": "My-Wallet-HD",
  "version": "0.5.0",
  "description": "Blockchain.info JavaScript Wallet",
  "homepage": "https://github.com/blockchain/My-Wallet-HD",
  "bugs": {
    "url": "https://github.com/blockchain/My-Wallet-HD/issues"
  },
  "license": "ISC",
  "author": {
    "name": "Blockchain.info",
    "url": "https://blockchain.info"
  },
  "contributors": [
    "Tim Lee",
    "Mark Pflueger",
    "Sjors Provoost",
    "Ben Reeves"
  ],
  "main": "wallet.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockchain/My-Wallet-HD.git"
  },
  "scripts": {
    "postinstall": "mkdir -p build && node_modules/bower/bin/bower install && browserify -s Browserify browserify-imports.js > build/browserify.js && cd node_modules/bip39 && npm run compile && mv bip39.js ../../build && cd ../.. && cp node_modules/xregexp/xregexp-all.js build && cd node_modules/sjcl && ./configure --with-sha1 && make && cd - && cp node_modules/sjcl/sjcl.js build",
    "test": "node ./node_modules/karma/bin/karma start karma.conf.js"
  },
  "dependencies": {
    "assert": "1.3.*",
    "bigi": "1.4.*",
    "bip39": "2.1.*",
    "bitcoinjs-lib": "1.5.*",
    "bs58": "2.0.*",
    "json-buffer": "2.0.*",
    "sha256": "0.1.*",
    "sjcl": "1.0.*",
    "unorm": "^1.3.3",
    "xregexp": "2.0.*"
  },
  "devDependencies": {
    "bower": "~1.3.12",
    "browserify": "~8.1.1",
    "coffee-script": "~1.8.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-uglify": "^0.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-env": "^0.4.4",
    "grunt-preprocess": "^4.1.0",
    "grunt-shell": "^1.1.2",
    "grunt-shrinkwrap": "^0.1.1",
    "karma": "~0.12.31",
    "karma-chrome-launcher": "~0.1.7",
    "karma-coffee-preprocessor": "~0.2.1",
    "karma-coverage": "^0.2.7",
    "karma-jasmine": "~0.3.5",
    "shelljs": "^0.3.0"
  }
}
