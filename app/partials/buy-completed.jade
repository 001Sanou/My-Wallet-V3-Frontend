div(translate-namespace="{{::completedTrade.namespace}}")
  h4
    i.security-red.mrs(ng-class="{'{{::completedTrade.icon}}': completedTrade.icon, success: !completedTrade.error}")
    span(translate=".TITLE")
  p.em-300.ptm(translate=".BODY" translate-values="::completedTrade.values")
  ul.em-300(ng-if="completedTrade.txProps")
    li.pts(ng-repeat="(prop, val) in completedTrade.txProps track by $index")
      span(translate="{{::prop}}")
      | :&nbsp;
      b(ng-bind="::val")
