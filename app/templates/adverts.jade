ul.aside-list.pln.pas.mtm(ng-show="ads && ads.length > 0")
  li.ad(ng-repeat="ad in ads")
    a.flex-between.flex-center.right-align.mbs(ng-click="visit(ad)")
      button.pan.btn.ad-btn(ng-click="visit(ad)", ng-class="[ ad.button_class ]")
        img.ad-btn-image(ng-src="{{ ad.image }}")
      span.adtext(ng-class="[ ad.button_class ]") {{ ad.title }}
